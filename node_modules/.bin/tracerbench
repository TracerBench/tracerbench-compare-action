#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/cthoburn/github/tracerbench-compare-action/node_modules/.pnpm/tracerbench@8.0.1_@types+node@22.13.1_typescript@4.9.5/node_modules/tracerbench/bin/node_modules:/Users/cthoburn/github/tracerbench-compare-action/node_modules/.pnpm/tracerbench@8.0.1_@types+node@22.13.1_typescript@4.9.5/node_modules/tracerbench/node_modules:/Users/cthoburn/github/tracerbench-compare-action/node_modules/.pnpm/tracerbench@8.0.1_@types+node@22.13.1_typescript@4.9.5/node_modules:/Users/cthoburn/github/tracerbench-compare-action/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/cthoburn/github/tracerbench-compare-action/node_modules/.pnpm/tracerbench@8.0.1_@types+node@22.13.1_typescript@4.9.5/node_modules/tracerbench/bin/node_modules:/Users/cthoburn/github/tracerbench-compare-action/node_modules/.pnpm/tracerbench@8.0.1_@types+node@22.13.1_typescript@4.9.5/node_modules/tracerbench/node_modules:/Users/cthoburn/github/tracerbench-compare-action/node_modules/.pnpm/tracerbench@8.0.1_@types+node@22.13.1_typescript@4.9.5/node_modules:/Users/cthoburn/github/tracerbench-compare-action/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tracerbench/bin/run" "$@"
else
  exec node  "$basedir/../tracerbench/bin/run" "$@"
fi
